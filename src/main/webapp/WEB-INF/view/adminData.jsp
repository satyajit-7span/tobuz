function editPackage (id){
	
 var getUrl = window.location;
	 
	  var url = getUrl .protocol + "//" + getUrl.host + "/getTobuzPackagesById" ;
	
		$.ajax({
		    url:url+"/" + id,
		    type: "GET",
		    dataType: 'json',
		      success: function(data) {
		    	 	$("#packageId").val(id);
		        	$("#countryName").val(data[0].countryName);
		        	$("#packageRole").val(data[0].userRole);
		        	$("#packageType").val(data[0].packageType);
		        	$("#description").val(data[0].description);
		        	$("#cost").val(data[0].cost);
		        	$("#months").val(data[0].expiryPeriodInMonths);
		        	$("#sequence").val(data[0].sequence);
		        	$("#alCount").val(data[0].advertListCount);
		        	$("#fileCounts").val(data[0].fileUploadCount);
		        	$("#contacts").val(data[0].noOfContactAccess);
		        	
		      },
		      error: function(jqXHR, textStatus, errorThrown) {
		        console.log(textStatus, errorThrown);
		      }
		}); 
	
	 $("#myModal").modal("show");
}

function saveChanges (){
	var id = $("#packageId").val();
	var countryName = $("#countryName").val();
	var userRole = $("#packageRole").val();
	var packageType = $("#packageType").val();
	var description = $("#description").val();
	var cost = $("#cost").val();
	var expiryPeriodInMonths = $("#months").val();
	var sequence = $("#sequence").val();
	var advertListCount = $("#alCount").val();
	var fileUploadCount = $("#fileCounts").val();
	var noOfContactAccess = $("#contacts").val();
	
	 var obj = {"id":id,"countryName":countryName,"userRole":userRole, 
			 "packageType":packageType, "description":description,
			 "cost":cost,"expiryPeriodInMonths":expiryPeriodInMonths,"sequence":sequence,
			 "advertListCount":advertListCount,"fileUploadCount":fileUploadCount,
			 "noOfContactAccess":noOfContactAccess };

	 var getUrl = window.location;
	  var url = getUrl .protocol + "//" + getUrl.host + "/updateAdminPackage" ;
	  $.ajax({
	        type: "POST",
	        url: url,
	        data: JSON.stringify(obj),
	        contentType: "application/json",
